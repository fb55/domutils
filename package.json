{
  "name": "domutils",
  "version": "1.2.0",
  "description": "utilities for working with htmlparser2's dom",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "node tests/00-runtests.js && jshint *.js tests/**/*.js lib/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/FB55/domutils.git"
  },
  "keywords": [
    "dom",
    "htmlparser2"
  ],
  "dependencies": {
    "domelementtype": "1"
  },
  "devDependencies": {
    "htmlparser2": "~3.3.0",
    "domhandler": "2",
    "jshint": "~2.3.0"
  },
  "author": "Felix Boehm <me@feedic.com>",
  "jshintConfig": {
    "proto": true,
    "unused": true,
    "eqnull": true,
    "undef": true,
    "quotmark": "double",
    "eqeqeq": true,
    "trailing": true,
    "node": true
  }
}
